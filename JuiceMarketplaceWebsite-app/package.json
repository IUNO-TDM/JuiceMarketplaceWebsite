    "moment": "^2.20.1",
    "morgan": "~1.9.0",
    "ng2-google-charts": "^3.4.0",
    "ngx-cookieconsent": "^1.0.1",
    "ngx-i18nsupport": "^0.17.0",
    "ngx-md": "^3.1.1",
